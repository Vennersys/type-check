{
  "name": "@leewinter/type-check",
  "version": "0.3.5",
  "description": "Library for checking js object property types",
  "type": "module",
  "module": "./dist/@leewinter/type-check.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@leewinter/type-check.js"
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf build && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json README.md LICENSE build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts",
    "prepare": "husky && echo 'npx lint-staged' > .husky/pre-commit && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "author": "lee.winter@boxspark.co.uk",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.0.4",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.3.5",
    "vitest": "^2.0.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.15.0"
  },
  "sideEffects": false
}
